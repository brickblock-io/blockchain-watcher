[ignore]
  .*/.yarn-cache/.*
  .*/node_modules/.*/test/.*

[include]

[libs]
  flow-typed

[lints]

[options]
  module.name_mapper='^eth/\(.*\)$' -> '<PROJECT_ROOT>/src/eth/\1'
  module.name_mapper='^jobs/\(.*\)$' -> '<PROJECT_ROOT>/src/jobs/\1'
  module.name_mapper='^test/\(.*\)$' -> '<PROJECT_ROOT>/src/test/\1'
  module.name_mapper='^types/\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'
  module.name_mapper='^utils/\(.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
  suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
  suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
  suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
  traces=1

[strict]
